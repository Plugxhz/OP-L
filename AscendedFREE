local MiscSection = MiscTab:CreateSection("Server Utilities")

local Button = MiscTab:CreateButton({
    Name = "Rejoin",
    Callback = function()
        local TeleportService = game:GetService("TeleportService")
        local Players = game:GetService("Players")
        local LocalPlayer = Players.LocalPlayer

        local Success, ErrorMessage = pcall(function()
            TeleportService:Teleport(game.PlaceId, LocalPlayer)
        end)

        if ErrorMessage and not Success then
            warn(ErrorMessage)
        end
    end
})

local MiscSection = PlayerTab:CreateSection("Player Utilities")

-- Função de ir para o jogador
local GoToPlayerInput = PlayerTab:CreateInput({
    Name = "Go to Player",
    CurrentValue = "",
    PlaceholderText = "Digite o nome do jogador",
    RemoveTextAfterFocusLost = false,
    Flag = "Input1",
    Callback = function(txt)
        local p1 = game.Players.LocalPlayer.Character.HumanoidRootPart
        local p2 = txt

        if game.Players:FindFirstChild(p2) then
            p1.CFrame = game.Players[p2].Character.HumanoidRootPart.CFrame
        else
            print("Jogador não encontrado!")
        end
    end,
})

-- Função de ir e voltar
local GoAndBackInput = PlayerTab:CreateInput({
    Name = "Go and Back",
    CurrentValue = "",
    PlaceholderText = "Digite o nome do jogador",
    RemoveTextAfterFocusLost = false,
    Flag = "Input2",
    Callback = function(txt)
        local p1 = game.Players.LocalPlayer.Character.HumanoidRootPart
        local p2 = txt
        local pos = p1.CFrame

        if game.Players:FindFirstChild(p2) then
            p1.CFrame = game.Players[p2].Character.HumanoidRootPart.CFrame
            wait(1)
            p1.CFrame = pos
        else
            print("Jogador não encontrado!")
        end
    end,
})

